<p id="notice"><%= notice %></p>

<h1>Listing Teachers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Skype</th>
      <th>Qq</th>
      <th>Country</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers.each do |teacher| %>
      <tr>
        <td><%= teacher.first_name %></td>
        <td><%= teacher.last_name %></td>
        <td><%= teacher.description %></td>
        <td><%= teacher.price %></td>
        <td><%= teacher.skype %></td>
        <td><%= teacher.qq %></td>
        <td><%= teacher.country %></td>
        <td><%= teacher.subject %></td>
        <td><%= link_to 'Show', teacher, class: "btn btn-link" %></td>
        <td><%= link_to 'Edit', edit_teacher_path(teacher), class: "btn btn-link" %></td>
        <td><%= link_to 'Destroy', teacher, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-link" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher', new_teacher_path, class: "btn btn-link" %>
